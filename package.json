  },
  "dependencies": {
    "async-file": "^2.0.2",
    "debug": "^2.6.0",
    "matrix-appservice-bridge": "^1.4.0a",
    "matrix-puppet-bridge": "^2.0.0",
    "npm": "^5.3.0",
    "read": "^1.0.7",
    "typed-promisify": "^0.4.0"
  },
  "devDependencies": {
    "typescript": "^2.3.3",
    "@types/node": "^7.0.22"
  }
